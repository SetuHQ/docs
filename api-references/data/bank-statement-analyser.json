                    "timestamp": {
                        "title": "Timestamp",
                        "type": "string",
                        "format": "date-time"
                    },
                    "ver": { "title": "Ver", "type": "string", "default": "1" },
                    "txnid": {
                        "title": "Txnid",
                        "type": "string",
                        "default": ""
